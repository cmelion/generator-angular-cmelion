{
  "generate": {
    "options": {
      "basePath": "<%= yeoman.dist %>/",
      "cache": [
        "<%= yeoman.dist %>/scripts/scripts.js",
        "<%= yeoman.dist %>/styles/main.css"
      ],
      "network": [
        "*",
        "http://*",
        "https://*"
      ],
      "exclude": [
        "js/jquery.min.js"
      ],
      "preferOnline": true,
      "verbose": true,
      "timestamp": true,
      "hash": false,
      "master": [
        "index.html"
      ]
    },
    "src": [
      "**/*.html",
      "<%= yeoman.dist %>/scripts/**/*.js",
      "<%= yeoman.dist %>/styles/**/*.css",
      "*.{ico,png,txt}",
      "assets/images/**/*",
      "assets/fonts/**/*"
    ],
    "dest": "<%= yeoman.dist %>/manifest.appcache"
  },
  "yeoman": "{<%= bugfix %>}"
}
