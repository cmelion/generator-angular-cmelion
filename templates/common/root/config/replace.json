{
  "baseHref": {
    "src": [
      "dist/index.html"
    ],
    "overwrite": true,
    "replacements": [
      {
        "from": "base href=\"/\"",
        "to": "base href=\"<%= gitinfo.remote.origin.url.substr(gitinfo.remote.origin.url.lastIndexOf(\"/\")).replace(\".git\",\"/\") %>\""
      }
    ]
  }
}